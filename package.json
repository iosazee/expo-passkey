{
  "name": "expo-passkey",
  "version": "0.1.0",
  "type": "module",
  "description": "Passkey authentication for Expo apps with Better Auth integration",
  "author": "Zee Mudia",
  "license": "MIT",
  "keywords": [
    "expo",
    "passkey",
    "biometric",
    "authentication",
    "better-auth",
    "react-native",
    "face-id",
    "touch-id",
    "fingerprint"
  ],
  "homepage": "https://github.com/iosazee/expo-passkey",
  "repository": {
    "type": "git",
    "url": "https://github.com/iosazee/expo-passkey.git"
  },
  "bugs": {
    "url": "https://github.com/iosazee/expo-passkey/issues"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "require": "./dist/cjs/client/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "require": "./dist/cjs/server/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/cjs/utils/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/cjs/types/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "client": [
        "./dist/client/index.d.ts"
      ],
      "server": [
        "./dist/server/index.d.ts"
      ],
      "utils": [
        "./dist/utils/index.d.ts"
      ],
      "types": [
        "./dist/types/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:client": "jest --selectProjects client",
    "test:server": "jest --selectProjects server",
    "test:watch": "jest --watch",
    "test:client:watch": "jest --selectProjects client --watch",
    "test:server:watch": "jest --selectProjects server --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier -w .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky && npm run build",
    "prepublishOnly": "npm run test && npm run lint && npm run build",
    "release": "changeset",
    "release:major": "changeset add --major",
    "release:minor": "changeset add --minor",
    "release:patch": "changeset add --patch",
    "version": "changeset version",
    "publish": "npm run build && changeset publish"
  },
  "peerDependencies": {
    "better-auth": "^1.0.0",
    "@better-fetch/fetch": "^1.0.0",
    "expo": ">=50.0.0",
    "expo-application": ">=5.0.0",
    "expo-local-authentication": ">=13.0.0",
    "expo-secure-store": ">=12.0.0",
    "expo-crypto": ">=12.0.0",
    "expo-device": ">=5.0.0",
    "react-native": "*",
    "zod": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "expo": {
      "optional": true
    },
    "expo-application": {
      "optional": true
    },
    "expo-local-authentication": {
      "optional": true
    },
    "expo-secure-store": {
      "optional": true
    },
    "expo-crypto": {
      "optional": true
    },
    "expo-device": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@eslint/js": "^9.22.0",
    "@jest/globals": "^29.7.0",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.6",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.6",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "zod": "^3.24.2",
    "expo": "^52.0.39",
    "expo-application": "^6.0.2",
    "expo-local-authentication": "^15.0.2",
    "expo-secure-store": "^14.0.1",
    "expo-crypto": "^14.0.2",
    "expo-device": "^7.0.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 100,
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  }
}
