{
  "name": "expo-passkey",
  "version": "0.1.0-beta.4",
  "type": "module",
  "description": "Passkey authentication for Expo apps with Better Auth integration",
  "author": "Zee Mudia",
  "license": "MIT",
  "keywords": [
    "expo",
    "passkey",
    "biometric",
    "authentication",
    "better-auth",
    "react-native",
    "face-id",
    "touch-id",
    "fingerprint"
  ],
  "homepage": "https://github.com/iosazee/expo-passkey",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iosazee/expo-passkey.git"
  },
  "bugs": {
    "url": "https://github.com/iosazee/expo-passkey/issues"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "require": "./dist/cjs/client/index.cjs",
      "default": "./dist/client/index.js",
      "node": null
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "require": "./dist/cjs/server/index.cjs",
      "default": "./dist/server/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/cjs/types/index.cjs",
      "default": "./dist/types/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "client": [
        "./dist/client/index.d.ts"
      ],
      "server": [
        "./dist/server/index.d.ts"
      ],
      "types": [
        "./dist/types/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:client": "jest --selectProjects client",
    "test:server": "jest --selectProjects server",
    "test:watch": "jest --watch",
    "test:client:watch": "jest --selectProjects client --watch",
    "test:server:watch": "jest --selectProjects server --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier -w .",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint:fix && npm run build"
  },
  "peerDependencies": {
    "better-auth": "^1.0.0",
    "@better-fetch/fetch": "^1.0.0",
    "expo": ">=50.0.0",
    "expo-application": ">=5.0.0",
    "expo-local-authentication": ">=13.0.0",
    "expo-secure-store": ">=12.0.0",
    "expo-crypto": ">=12.0.0",
    "expo-device": ">=5.0.0",
    "react-native": "*",
    "zod": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "expo": {
      "optional": true
    },
    "expo-application": {
      "optional": true
    },
    "expo-local-authentication": {
      "optional": true
    },
    "expo-secure-store": {
      "optional": true
    },
    "expo-crypto": {
      "optional": true
    },
    "expo-device": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "tag": "beta"
  }
}
